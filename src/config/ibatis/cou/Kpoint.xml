		                  COU_KPOINT_TBL.NAME like  CONCAT('%',#searchKey#,'%')
			    </isNotEqual>
	        </isNotNull>
	        <isNotNull property="courseId">
            	 <isGreaterThan prepend="and" property="courseId" compareValue="0">
                		COU_KPOINT_TBL.COURSE_ID=#courseId#
                </isGreaterThan>
	        </isNotNull>
        </select>

         
